module Rogue

enum ControlType( control_type_name:String )
  UNDEFINED( "undefined" )
  BLOCK( "block" )
  CATCH( "catch" )
  CONTINGENT( "contingent" )
  FOR_EACH( "forEach" )
  IF( "if" )
  LOCAL_SCOPE( "local scope" )
  LOOP( "loop" )
  NECESSARY( "necessary" )
  RETURN( "procedure" )
  SUFFICIENT( "sufficient" )
  THROW( "throw" )
  TRY( "try" )
  USE( "use" )
  WHICH( "which" )
  WHILE( "while" )

  METHODS
    method is_defined->Logical
      return (this != ControlType.UNDEFINED)
endEnum
