      local macro_args = (clone_args as MacroArgs)
      if (macro_args)
        return macro_args.inline_write_local( this )
      else
        local task_args = (clone_args as TaskArgs)
        if (task_args)
          return task_args.cmd_write( t,local_info, new_value.cloned( clone_args, new_t ) )
        endIf
