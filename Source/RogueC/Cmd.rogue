      else
        return this
      endIf

    method resolve_for_reference( scope:Scope, left_type:Type, right_type:Type, force_error=true:Logical )->Cmd
      local cmd = resolve_operator_method( "operator<", scope, left, left_type, right, right_type, &is_optional )
