        writer.print cmd.as_type.c_ref_name
        writer.print ")"
        writer.print( "Rogue_as(" )
        visit( cmd.operand )
        writer.print( ", " ).print( cmd.as_type.id ).print( ')' )
        writer.print ")"
      endIf

