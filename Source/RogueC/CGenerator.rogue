    method on_visit( cmd:Is )
      if (cmd.left.type is not cmd.right.type)
        writer.print "(void*)"
        print_parenthesized( cmd.left )
        writer.print " == "
        writer.print "(void*)"
        print_parenthesized( cmd.right )
      else
        print_parenthesized( cmd.left )
