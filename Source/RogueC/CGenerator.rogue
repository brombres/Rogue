      if (cmd.args.count == 0)
        writer.print '('
        print_with_cast( "THIS", this_type, info.type_context )
        writer.print ')'
      else
        writer.print "( "
        print_with_cast( "THIS", this_type, info.type_context )
        if (cmd.args.count)
          forEach (arg at i in cmd.args)
            writer.print ", "
