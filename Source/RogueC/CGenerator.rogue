      forEach (cmd_case in cmd.cases)
        forEach (arg in cmd_case.args)
          writer.print "case "
          visit( arg )
          writer.println ':'
        endForEach
        writer.println '{'
        writer.indent  += 2
        visit( cmd_case.statements )
        if (cmd_case.statements.count and cmd_case.statements.last not instanceOf Return) writer.println "break;"
        writer.indent  -= 2
        writer.println '}'
      endForEach
      if (cmd.statements)
        writer.println "default:"
        writer.println '{'
        writer.indent  += 2
