          case Program.type_Byte:      writer.print( "as_bytes[" )
          case Program.type_Logical:   writer.print( "as_logicals[" )
          others
            throw t.error( "[INTERNAL] Unhandled type in CmdReadArrayElement." )
        endWhich
        index.write_cpp( writer )
        writer.print( "]" )

      else
