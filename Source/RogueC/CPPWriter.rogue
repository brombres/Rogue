          # Write the else/if in the "else if" style.
          writer.print( "else " )
          else_statements.write_cpp( writer, &skip_trace_update )
        else
          # General 'else' block.
          writer.println( "else" )
          writer.println( "{" )
          writer.indent += 2
          else_statements.write_cpp( writer )
          writer.indent -= 2
          writer.println( "}" )
