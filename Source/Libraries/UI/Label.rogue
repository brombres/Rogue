library UI

class Label : Component
  METHODS
    method init( text:String )
      init( Text(text) )

    method init( content:Component )
      set_content( content )

    method init_object
      alignment = Anchor.CENTER

    method set_text( text:String )
      content = Text( text )

    method set_text( text:Text )
      content = text
endClass

